{
  "name": "black",
  "productName": "Black CTIS Viewer",
  "version": "1.1.0",
  "description": "An digital master/mistress",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=releases",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=releases --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Black CTIS Viewer\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=releases"
  },
  "author": "Galactic Cat",
  "license": "Apache-2.0",
  "dependencies": {
    "electron-config": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^1.6.11"
  }
}
